<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bernardo.kapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AulasActivity"
            android:label="@string/title_activity_aulas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FestasActivity"
            android:label="@string/title_activity_festas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FestivaisActivity"
            android:label="@string/title_activity_festivais"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
